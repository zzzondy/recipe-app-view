<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home_destination">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.recipeapp.BlankFragment"
        android:label="blankFragment"
        tools:layout="@layout/fragment_blank">

    </fragment>
    <fragment
        android:id="@+id/home_destination"
        android:name="com.recipeapp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/profile_destination"
        android:name="com.recipeapp.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/addingFragment"
        android:name="com.recipeapp.AddingFragment"
        android:label="fragment_adding"
        tools:layout="@layout/fragment_adding" />

</navigation>